{
  "compilerOptions": {
      "sourceMap": true,
      "target": "es2015",
      "module": "es2015",
      "moduleResolution": "node",
      "noEmitOnError": true,
      "lib": [
          "es2017",
          "DOM"
      ],
      "strict": false,
      "esModuleInterop": true,
      "outDir": "out-tsc",
      "rootDir": "./",
      "allowJs": true,
      "noImplicitAny": true
  },
  "include": [
      "./src/**/*.ts"
  ]
}